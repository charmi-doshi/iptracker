<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddStudentCont</servlet-name>
  	<display-name>AddStudentCont</display-name>
  	<description></description>
  	<servlet-class>com.controller.AddStudentCont</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentListCont</servlet-name>
  	<display-name>StudentListCont</display-name>
  	<description></description>
  	<servlet-class>com.controller.StudentListCont</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddStudentCont</servlet-name>
  	<url-pattern>/AddStudentCont</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentListCont</servlet-name>
  	<url-pattern>/StudentListCont</url-pattern>
  </servlet-mapping>
</web-app>
